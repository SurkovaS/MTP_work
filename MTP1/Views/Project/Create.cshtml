@model MTP1.Models.Project

@{
    ViewBag.Title = string.Format("Создание проекта тестирования | {0}", MTP1.Constants.TITLE);
}

<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>


@using (Html.BeginForm())
{
    
    <fieldset>
        <legend>Создание проекта тестирования</legend>
        @Html.ActionLink("Отменить", "Index")
        @Html.EditorForModel()
        <p>
            <input type="submit" value="Сохранить" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Проекты", "Index")
</div>
